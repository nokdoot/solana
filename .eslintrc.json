{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": "off"
    },
    "plugins": [
        "jsdoc"
    ],
    "extends": [
        "eslint:recommended", 
        "plugin:jsdoc/recommended",
        "plugin:jsdoc/recommended-error"
    ]
}
